{
  "manifest_version": 3,
  "name": "Secure Web Authentication",
  "description": "Enhance authentication cookie security using a random forest model",
  "background": { "service_worker": "background.bundle.js"},
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["content.styles.css", "icon-128.png", "icon-34.png"],
      "matches": []
    }
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "cookies",
    "tabs"
  ]
}
